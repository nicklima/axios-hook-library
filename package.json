{
  "name": "axios-hook-library",
  "version": "1.0.2",
  "description": "NPM React Hook Package to fetch data with Axios",
  "author": "Nick Lima",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicklima/axios-hook-library.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "preact",
    "library",
    "module",
    "package",
    "create-react-app",
    "cli",
    "component",
    "rollup",
    "babel",
    "publish"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.tsx\" \"example/src/**/*.tsx\"",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint ./src/index.tsx",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "example": "cd example && npm install",
    "preview": "cd example && npm run start",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "prepublishOnly": "npm run test",
    "publish": "npm publish",
    "preversion": "npm run test:lint",
    "version": "npm run format && git add -A && git commit -m \"ðŸ”– updating %s\"",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.8",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "axios-mock-adapter": "^1.19.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-standard": "^4.1.0",
    "gh-pages": "^3.1.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}